/*--------------grid-------------------*/
.grid{width:100%;background:#fff;border-top:1px #dddddd solid;border-left:1px #dddddd solid;}
.grid .thline{background:url(../images/table-th-bg.png) 0 0 repeat-x;}
.grid th{text-align: center;background:#f0f5f8;height:40px;line-height:40px;border-bottom:1px #dddddd solid;border-right:1px #cccccc solid;font-size:12px;font-weight:bold;color:#555;padding:0 5px;overflow:hidden;}
.grid tr.color{background:#f9f9f9;}
.grid td{line-height:40px;text-align:center;font-size:12px;color:#555;border-bottom:1px #dddddd solid;border-right:1px #dddddd solid;}
.grid th.brn{border-right:none;}
.grid td.bbn{border-bottom:none;}
.grid td.brn{border-right:none;}
.grid .bgcolor-f{background:#fff;font-weight:lighter;}
.grid td.bgcolor{background:#f0f5f8;font-weight:bold;}
.grid th.w5{width:5%;}
.grid td.w3{width:2.5%;}
.grid th.w44{width:44px;}
.grid th.w328{width:328px;}
.grid th.w233{width:233px;}
.grid td{height:40px;line-height:40px;font-size:12px;color:#444;text-align:center;padding:0 10px;border-bottom:1px #e1e1e1 solid;border-right:1px #e1e1e1 solid;}
.grid th.tb_index{text-align:center;}
.grid td.tb_index{text-align:center;}
/*.grid th.tb_last{border-right:none;}
.grid td.tb_last{border-right:none;}*/
/*.grid th.brn{border-right:none;}*/
.grid td.left{text-align:left;padding-left:20px;}
.grid td.bbn{border-bottom:none;}
.grid td.brn{border-right:none;}

tr.trodd{background-color:#f7fafb;}
/*tr.trlast td{border-bottom:none;}*/

td.tdseq{font-weight:bold;}
.pagination{
	height:43px;
	padding:0 10px 0 18px;
	background:url(../images/entry-page-bg.png) 0 0 repeat-x;
	border:1px #dddddd solid;
	border-top:none;
}
.pagination .entry{float:left;height:24px;line-height:24px;margin-top:10px;overflow:hidden;}
.pagination .entry span{float:left;font-size:12px;color:#999;display:inline;}
.pagination .entry span em{font-weight:bold;color:#333;}
.pagination .entry .mr25{margin-right:25px;}
.pagination .page{
	margin-top:10px;
}
.setting-form .error { position: absolute; left: 235px; font-size: 12px; word-break: keep-all; white-space: nowrap; padding-left: 31px; background: url(../images/wrong-bg2_01.gif) 0 0 no-repeat; display: inline-block; line-height: 28px; }
.setting-form .error span { color: #BB0000; height: 28px; line-height: 28px; padding: 0 8px 0 5px; background: url(../images/wrong-bg2_03.gif) right 0 no-repeat #ffe8e8; display: inline-block; }

#login-area .input-box .error{
	background: url("../images/wrong-bg2.png") no-repeat scroll 0 0 transparent;    
    position: absolute;
    left:235px;
    font-size: 12px;
    word-break: keep-all;
    white-space:nowrap;
    padding-left:31px;
	background:url(../images/wrong-bg2_01.gif) 0 0 no-repeat;
	display: inline-block;
    line-height: 28px;
}
#login-area .input-box .error span{
	color: #BB0000;
	height:28px;
	line-height: 28px;

body{margin:0px;padding:0px;font-family:"微软雅黑",Arial, Tahoma,Verdana,sans-serif; font-size:12px; }
ul{margin:0px;padding:0px;}
li{margin:0px;padding:0px; list-style:none;}
h1{margin:0px;padding:0px;}
h2{margin:0px;padding:0px;}
h3{margin:0px;padding:0px;}
h4{margin:0px;padding:0px;}

dl{margin:0px;padding:0px;}
dd{margin:0px;padding:0px;}
dt{margin:0px;padding:0px;}
img{border:0px;}
span{margin:0px;padding:0px;}
p{margin:0px;padding:0px;}
a{color:#292929; text-decoration:none;blr:expression(this.onFocus=this.blur())}
a:visited{color:#292929; text-decoration:none;}
a:hover{color:#ff3600; text-decoration:none;}
em{ color:#ff6434; font-style:normal;}
.clear{clear: both;display: block;font-size: 1px;height: 0;line-height: 0;visibility: hidden;width: 1px;}

#container{width:1000px; margin:0px auto;}
/*头部*/
#header{width:1000px;float:left;height:426px;}
/*logo区域*/
#header .top{width:100%;height:70px; float:left;display:block;}
#header .top a{ float:right; display:block;color:#999999;margin-left:10px;margin-right:10px;margin-top:25px;}
#header .top a:hover{color:#ffffff;}
#header a.logo{ float:left; margin:0;}
/*登录区域*/
#header .login{width:100%;height:50px; float:left;margin-top:306px;}
#header .login .lo{ background:url(../images/btn_01.gif) no-repeat;width:132px;height:41px; float:right;margin-left:28px;}
#header .login .lo:hover{background:url(../images/btn_01h.gif) no-repeat;}
#header .login .gr{ background:url(../images/btn_02.gif) no-repeat;width:132px;height:41px; float:right;}
#header .login .gr:hover{background:url(../images/btn_02h.gif) no-repeat;}
/*主体*/
.main{width:100%; float:left;}
.main span{ display:block; float:left;width:500px;height:250px;margin-top:35px;border-bottom:1px #e6e6e6 solid;}
.main p{display:block; float:left;width:480px; float:left;font-size:14px;line-height:25px; color:#808080;font-weight:bold;margin-top:15px;}
.main img.tp{ float:right;}
.main h1{color:#454981;display:block; float:left;width:480px;font-size:22px;}

/*底部区域*/
#footer{width:100%; float:left;height:50px;line-height:40px; text-align:center; color:#b3b3b3;}

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
		<title>数组</title>
		<script type="text/javascript" src="../../commons/common.js"></script>
		<script type="text/javascript">
			//字面量
			var empty=[]
			var numbers=[1,2,3,4]
			//长度
			console.info(numbers.length)
			numbers[10]=true
			console.info(numbers.length)//11
			console.info(numbers)//[1,2,3,4,undefined,undefined,undefined,undefined,undefined,undefined,true]
			numbers.length=3
			console.info(numbers)
			numbers.length=5
			console.info(numbers)
			
			//删除
			delete numbers[1]
			console.info(numbers)
			numbers.splice(1,1)
			console.info(numbers)
			
			//枚举
			for(var i = 0; i< numbers.length;i++){
				console.info(numbers[i])
			}
			
			//增加方法
			Array.method('reduce',function(f,value){
				var i;
				for(i=0;i<this.length;i++){
					value=f(this[i],value)
				}
				return value
			})
			var data=[1,2,3,4]
			var sum=data.reduce(function(a,b){return a+b},0)
			console.info(sum)
		</script>
	</head>
	<body>
	</body>

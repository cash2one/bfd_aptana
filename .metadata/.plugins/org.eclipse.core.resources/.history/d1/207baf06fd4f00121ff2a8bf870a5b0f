        <script type="text/javascript">
            function changeTab(a){
                var lis = document.getElementById('nav').getElementsByTagName("li");
                //alert("111");
                for (var i = 0; i < lis.length; i++) {
                    if (lis[i] == a) {
                    
                        lis[i].className = "current";
                    }
                    else {
                        lis[i].className = "";
                        
                    }
                }
            }
        </script>

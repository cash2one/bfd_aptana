.ls {
    position: relative;
    height: 160px;
    width: 160px;
}
.ls .mark{
	z-index:50;
	position:absolute;
	bottom:0px;
	height: 0px;
    width: 160px;
	color:#fff;
	overflow:hidden;
	background-color:#000;
	filter:alpha(opacity=40);  /*支持 IE 浏览器*/  
	-moz-opacity:0.40; /*支持 FireFox 浏览器*/  
	opacity:0.40;  /*支持 Chrome, Opera, Safari 等浏览器*/  
}
.ls .lsico {
	z-index:20;
	background-image: url(images/ico.png);
	background-repeat: no-repeat;
	height: 50px;
	width: 50px;
	text-align: center;
	line-height: 50px;
	position: absolute;
	right: 5px;
	bottom: 5px;
	color: #FFF;
	font-family: "微软雅黑";
	font-size: 11px;
}
.ls .lsico02 {
	background-image: url(images/ico.png);
